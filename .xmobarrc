Config { font = "-*-Fixed-Misc-*-*-*-*-13-*-*-*-*-*-*-*-",
		 bgColor = "black",
		 fgColor = "grey",
		 position = Static { xpos = 0, ypos = 0, width = 1840, height = 16 },
		 commands = [ Run Cpu ["-L", "10", "-H", "60", "--normal", "green", "--high", "red", "-p", "2"] 10,
		 			  Run DiskU [("sdb1", "Root: <usedp>%"), ("sda1", "Store: <usedp>%")] ["-L", "30", "-H", "80", "-m", "1", "-l", "green", "-n", "yellow", "-h", "red"] 60,
					  Run Memory ["-t", "Mem: <usedratio>%", "-H", "85", "-L", "50", "-l", "lightblue", "-n", "yellow", "-h", "red"] 10,
					  Run Date "%a %b %_d %l:%M" "date" 10,
					  --Run BatteryP ["BAT0"] ["-t", "Bat: <left>%" , "-L", "15", "-H", "50" , "-l", "red", "-n", "yellow", "-h", "green"] 10,
					  -- Script I generated myself - has latency because UPower doesn't update often enough.  Might fix.
					  --Run Com "python3" ["/home/zaphod/.xmobin/batt.py"] "battery" 10,
					  Run Com "/home/zaphod/.xmobin/music" [] "music" 10,
					  Run Com "/home/zaphod/.xmobin/volume" [] "volume" 1,
					  Run Com "/home/zaphod/.xmobin/netStatus" [] "net" 10,
					  Run StdinReader ],
		sepChar = "%",
		alignSep = "}{",
		template = "%StdinReader% }{ %music% %volume% | %net% | %cpu% | %memory% | %disku% --- <fc=#ee9a00>%date%</fc> "
		--template = "%StdinReader% }{ %music% %net% | %cpu% | %memory% | %disku% --- <fc=#ee9a00>%date%</fc> "
	  }
